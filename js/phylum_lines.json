    "y": {"aggregate":"count", "field": "phylum", "type": "nominal", "title": "Registered Entries"},
    "color": {"field": "phylum_large", "type": "nominal", "title": "Phylum"},
    "tooltip": [
    {"field": "period", "type": "nominal", "title": "Period"}]
